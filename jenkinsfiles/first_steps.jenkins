#!groovy
// check run project

pipeline {
    agent {
        docker {
            image 'python:3'
            label 'master'
        }
    }
    options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
        timestamps()
    }
    stages{
        stage("Run py") {
            steps {
                sh 'ls -l'
                sh 'python main.py'
            }
        }
    }
}